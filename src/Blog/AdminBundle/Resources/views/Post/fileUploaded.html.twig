<script type='text/template' id='file-template'>
    <tr class="template-download fade in">
        <td>
            <p class="name">
                <img src="__fileurl__" width="100"/>
                <a download="__filename__" title="__filename__" href="__fileurl__">

                    __filename__
                </a>
                <span class="message message-exceed" style="display: none"><i class="fa fa-thumbs-o-down"></i>{{ 'post.error_file_size' | trans }}</span>
                <span class="message message-limit" style="display: none"><i class="fa fa-thumbs-o-down"></i>{{ 'post.error_limit_files' | trans }}</span>
                <span class="message message-type" style="display: none"><i class="fa fa-thumbs-o-down"></i>{{ 'post.error_file_type' | trans }}</span>
                <span class="message message-min-resolution" style="display: none"><i class="fa fa-thumbs-o-down"></i>{{ 'post.error_min_resolution' | trans }}</span>
                <span class="message message-ok" style="display: none"><i class="fa fa-thumbs-o-up" style="color:green"></i>{{ 'post.file_upload_ok' | trans }}</span>
            </p>
        </td>
        <td>
            <span class="size">__filesize__</span>
            <div aria-valuenow="0" aria-valuemax="100" aria-valuemin="0" role="progressbar" class="progress progress-striped active">
                <div style="width:0%;" class="progress-bar progress-bar-success"></div>
            </div>
        </td>
        <td>
            <a rel="tooltip" title="Delete" data-action="delete" class="btn btn-sm bg-color-pinkDark txt-color-white" href="#" data-url="">
                <i class="fa fa-trash-o"></i>
            </a>
            <a rel="tooltip" title="Delete" data-action="delete-row" class="btn btn-sm bg-color-pinkDark txt-color-white" href="#" data-url="" style="display:none">
                <i class="fa fa-trash-o"></i>
            </a>
        </td>
    </tr>
</script>